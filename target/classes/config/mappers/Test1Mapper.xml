<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yhk.datasource.dao.Test1Dao">

    <resultMap id="test1Map" type="com.yhk.datasource.entity.Test1">
        <id column="id" property="id"/>
        <result column="user_name" property="userName"/>
        <result column="age" property="age"/>
    </resultMap>
    <select id="getById" parameterType="int" resultMap="test1Map">
        select *
        from test1
        where id = #{id}
    </select>
</mapper>
